    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Proosals.
    -->
    <changeSet id="20210102114819-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="visits_id"
                                 baseTableName="proosals"
